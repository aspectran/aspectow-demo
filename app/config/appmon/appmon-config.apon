instance: {
    name: root
    title: Root
    event: {
        name: activity
        target: root
        parameters: {
            +: /**
        }
    }
    event: {
        name: session
        target: tow.server/root
    }
    event: {
        name: status1
        title: Heap
        description: Shows JVM Heap memory usage status
        reader: com.aspectran.appmon.exporter.event.status.jvm.HeapMemoryUsageReader
        heading: true
        sampleInterval: 500
    }
    event: {
        name: status2
        title: Undertow TP
        description: Metrics of Undertow thread pool resources
        reader: com.aspectran.appmon.exporter.event.status.undertow.NioWorkerMetricsReader
        target: tow.server
        heading: true
        sampleInterval: 500
    }
    log: {
        name: app
        title: Root App
        file: /logs/root.log
        lastLines: 1000
        sampleInterval: 300
    }
    log: {
        name: access
        title: Root Access
        file: /logs/root-access.log
        lastLines: 1000
        sampleInterval: 300
    }
}
instance: {
    name: appmon
    title: AppMon
    hidden: true
    event: {
        name: activity
        target: appmon
        parameters: {
            +: /**
        }
    }
    event: {
        name: session
        target: tow.server/appmon
    }
    event: {
        name: status1
        title: Heap
        description: Shows JVM Heap memory usage status
        reader: com.aspectran.appmon.exporter.event.status.jvm.HeapMemoryUsageReader
        heading: true
        sampleInterval: 500
    }
    log: {
        name: appmon
        title: AppMon
        file: /logs/appmon.log
        lastLines: 1000
        sampleInterval: 300
    }
    log: {
        name: access
        title: AppMon Access
        file: /logs/appmon-access.log
        lastLines: 1000
        sampleInterval: 300
    }
}
